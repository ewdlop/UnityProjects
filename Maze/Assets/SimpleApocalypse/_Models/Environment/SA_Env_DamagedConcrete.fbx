			bStarted = true;
		}

		if (Data->Completed)
		{
			OutResult = (EAzureSpatialAnchorsResult)Data->Result;
			OutErrorString = Data->OutError;
			Data.Reset();
			Response.FinishAndTriggerIf(true, ExecutionFunction, OutputLink, CallbackTarget);
