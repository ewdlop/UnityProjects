version: 2.59

precision:   16
maxExponent: 384
minExponent: -383
extended:    1
clamp:       1
rounding:    half_even

dddvi001 divideint  1     1    ->  1
dddvi002 divideint  2     1    ->  2
dddvi003 divideint  1     2    ->  0
dddvi004 divideint  2     2  